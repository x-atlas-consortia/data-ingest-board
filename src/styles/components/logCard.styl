.c-logCard
  border solid 3px transparent
  margin 10px
  cursor pointer
  width 25%
  max-width 320px
  word-break break-all
  h3 
    small 
      font-size .5em
  +md-max()
    width 100%
    margin 0
    
  &.is-highlighted
    border-color $col-hover-blue
  &:hover
    background-color $col-dirty-white

  &__slickWrap
    margin-bottom 25px
  
  &__flexWrap 
    overflowY auto
    max-height 150px
    display flex 
    flex-wrap wrap

  &__date
    font-size 12px
    color grey

  .ant
    &-card
      &-head-title 
        padding-top 20px