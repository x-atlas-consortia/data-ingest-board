{
  "name": "data-ingest-board",
  "version": "1.4.20",
  "private": true,
  "watch": {
    "addons": "./lib/addons/core/*.js"
  },
  "scripts": {
    "all": "npm-run-all --parallel dev css sankey-css watch",
    "dev": "next dev --port 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "css": "stylus --include-css --compress -w styles/main.styl -o styles/main.css",
    "sankey-css": "stylus --include-css --compress -w styles/xac-sankey.styl -o public/css/xac-sankey.css",
    "addons": "node ./node_modules/xac-esbuild/index.js -o=public/js/ -s=lib/addons/core/ -outfile=public/js/addons.js",
    "watch": "npm-watch"
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "semi": false,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "antd": "^6.3.1",
    "axios": "^1.13.5",
    "bootstrap": "^5.3.8",
    "cookies-next": "^6.1.1",
    "d3": "^7.9.0",
    "d3-sankey": "^0.12.3",
    "next": "^16.1.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "prop-types": "^15.8.1",
    "react": "^19.2.4",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.4",
    "react-idle-timer": "^5.7.2",
    "react-infinite-scroll-component": "^6.1.1",
    "stylus": "^0.64.0",
    "xac-sankey": "github:x-atlas-consortia/data-sankey#1.0.18"
  },
  "devDependencies": {
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.0",
    "npm-watch": "^0.13.0",
    "xac-esbuild": "github:x-atlas-consortia/xac-esbuild#1.0.0"
  }
}
