{
  "name": "data-ingest-board",
  "version": "1.4.7",
  "private": true,
  "scripts": {
    "all": "npm-run-all --parallel dev css sankey-css addons",
    "dev": "next dev --port 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "css": "stylus --include-css --compress -w styles/main.styl -o styles/main.css",
    "sankey-css": "stylus --include-css --compress -w styles/xac-sankey.styl -o public/css/xac-sankey.css",
    "addons": "./node_modules/.bin/babel lib/addons/core/__polyfill.js lib/addons/core/Addon.js lib/addons/core/*.js --presets minify --out-file public/js/addons.js",
    "addons-watch": "watch 'npm run addons' ./lib/addons/core"
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "semi": false,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "antd": "^5.20.6",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.2",
    "cookies-next": "^3.0.0",
    "d3": "^7.9.0",
    "d3-sankey": "^0.12.3",
    "eslint-config-next": "^14.2.7",
    "next": "^14.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.9.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.3.1",
    "react-idle-timer": "^5.7.2",
    "stylus": "^0.59.0",
    "xac-sankey": "github:x-atlas-consortia/data-sankey#1.0.6a"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.24.7",
    "babel-preset-minify": "^0.5.2",
    "watch": "^1.0.2"
  }
}
